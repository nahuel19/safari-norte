@{
    ViewBag.Title = "Home Page";
}
<div style="width:100%">
    <div id="myCarousel" class="carousel slide">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <!-- Carousel items -->
        <div class="carousel-inner">
            <div class="active item"><img src="https://apasio-safari.com/wp-content/uploads/2018/12/KEN_2018_1TKI_06C5A8260_2000.jpg" width="700"></div>
            <div class="item"><img src="https://thumbor.forbes.com/thumbor/960x0/https%3A%2F%2Fblogs-images.forbes.com%2Fadriennejordan%2Ffiles%2F2019%2F07%2Fmu4.jpg" width="700" /></div>
            <div class="item"><img src="https://cdn.civitatis.com/tanzania/arusha/galeria/safari-elefante.jpg" width="700" /></div>
        </div>
        <!-- Carousel nav -->
        <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
        <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
    </div>
</div>


